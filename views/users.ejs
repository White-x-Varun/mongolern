<h2>All Users</h2>
<table border="1">
  <tr><th>Username</th><th>Email</th><th>Actions</th></tr>
  <% users.forEach(user => { %>
    <tr>
      <td><%= user.username %></td>
      <td><%= user.email %></td>
      <td>
        <a href="/edit/<%= user._id %>">Edit</a>
        <a href="/delete/<%= user._id %>" onclick="return confirm('Are you sure?')">Delete</a>
      </td>
    </tr>
  <% }) %>
</table>
<a href="/">Create New</a>
